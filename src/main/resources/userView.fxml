<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<TitledPane text="Your friends" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetwork.controller.User2Controller">
   <content>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <TableView fx:id="friendsTable" layoutX="23.0" layoutY="14.0" prefHeight="295.0" prefWidth="228.0">
              <columns>
                <TableColumn fx:id="tableColumnFirstName" prefWidth="75.0" text="First Name" />
                <TableColumn fx:id="tableColumnLastName" prefWidth="75.0" text="Last Name" />
                  <TableColumn fx:id="tableColumnEmail" prefWidth="75.0" text="Email" />
              </columns>
            </TableView>
            <Button layoutX="386.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleDeleteFriend" text="Delete friend" />
            <Button layoutX="379.0" layoutY="74.0" mnemonicParsing="false" onAction="#handleAddFriend" text="Add new friend" />
            <Button layoutX="395.0" layoutY="136.0" mnemonicParsing="false" text="Requests" />
         </children>
      </AnchorPane>
   </content>
</TitledPane>
